#: lib/error.c:56
msgid "success"
msgstr "успішно"

#: lib/error.c:60
msgid "out of memory"
msgstr "бракує пам’яті"

#: lib/error.c:64
msgid "could not determine locale encoding format"
msgstr "не вдалося визначити формат кодування мови"

#: lib/error.c:68
msgid "could not convert string to UTF-8"
msgstr "не вдалося перетворити рядок в UTF-8"

#: lib/error.c:72
msgid "string encoding error"
msgstr "помилка кодування рядка"

#: lib/error.c:76
msgid "string could not be NFC normalized"
msgstr "рядок не може бути нормалізований за NFC"

#: lib/error.c:80
msgid "string contains invalid punycode data"
msgstr "рядок містить недійсні дані punycode"

#: lib/error.c:84
msgid "punycode encoded data will be too large"
msgstr "дані з кодуванням punycode будуть занадто великими"

#: lib/error.c:88
msgid "punycode conversion resulted in overflow"
msgstr "перетворення punycode призвело до переповнення"

#: lib/error.c:92
msgid "domain name longer than 255 characters"
msgstr "доменне ім'я перевищує 255 символів"

#: lib/error.c:96
msgid "domain label longer than 63 characters"
msgstr "мітка домену перевищує 63 символів"

#: lib/error.c:100
msgid "input A-label is not valid"
msgstr "введення A-label неприпустиме"

#: lib/error.c:104
msgid "input A-label and U-label does not match"
msgstr "введення A-label та U-label не збігається"

#: lib/error.c:108
msgid "string is not in Unicode NFC format"
msgstr "рядок не є у форматі Unicode NFC"

#: lib/error.c:112
msgid "string contains forbidden two hyphens pattern"
msgstr "рядок містить заборонені двохдефісні зразки"

#: lib/error.c:116
msgid "string start/ends with forbidden hyphen"
msgstr "рядок починається/закінчується забороненим дефісом"

#: lib/error.c:120
msgid "string contains a forbidden leading combining character"
msgstr "рядок містить заборонений провідний комбінаційний символ"

#: lib/error.c:124
msgid "string contains a disallowed character"
msgstr "рядок містить заборонений символ"

#: lib/error.c:128
msgid "string contains a forbidden context-j character"
msgstr "рядок містить заборонений символ context-j"

#: lib/error.c:132
msgid "string contains a context-j character with null rule"
msgstr "рядок містить символ context-j з нульовим правилом"

#: lib/error.c:136
msgid "string contains a forbidden context-o character"
msgstr "рядок містить заборонений символ context-o"

#: lib/error.c:140
msgid "string contains a context-o character with null rule"
msgstr "рядок містить символ context-o з нульовим правилом"

#: lib/error.c:144
msgid "string contains unassigned code point"
msgstr "рядок містить непризначену кодову точку"

#: lib/error.c:148
msgid "string has forbidden bi-directional properties"
msgstr "рядок містить заборонені двонаправлені властивості"

#: lib/error.c:152
msgid "Unknown error"
msgstr "Невідома помилка"

#: src/idn2.c:59
#, c-format
msgid "Try `%s --help' for more information.\n"
msgstr "Виконайте команду «%s --help», щоб дізнатися більше.\n"

#: src/idn2.c:63
#, c-format
msgid "Usage: %s [OPTION]... [STRINGS]...\n"
msgstr "Використання: %s [ПАРАМЕТР]... [РЯДКИ]...\n"

#: src/idn2.c:66
msgid ""
"Internationalized Domain Name (IDNA2008) convert STRINGS, or standard "
"input.\n"
"\n"
msgstr ""
"Інтернаціоналізоване доменне ім'я (IDNA2008) перетворює STRINGS, чи "
"стандартне введення.\n"
"\n"

#: src/idn2.c:70
msgid ""
"Command line interface to the Libidn2 implementation of IDNA2008.\n"
"\n"
"All strings are expected to be encoded in the locale charset.\n"
"\n"
"To process a string that starts with `-', for example `-foo', use `--'\n"
"to signal the end of parameters, as in `idn2 --quiet -- -foo'.\n"
"\n"
"Mandatory arguments to long options are mandatory for short options too.\n"
msgstr ""
"Інтерфейс командного рядка до Libidn2 реалізації дляf IDNA2008.\n"
"\n"
"Програма вважає, що кодуванням всіх рядків є кодування локалі.\n"
"\n"
"Для обробки рядка, що починається з «-», наприклад, «-foo» додайте у команду "
"«--»\n"
"для позначення завершення параметрів. Приклад: «idn2 --quiet -a -- -foo».\n"
"\n"
"Обов’язкові аргументи для параметрів у розгорнутому записів є обов’язковими "
"і для\n"
"скорочених форм запису.\n"

#: src/idn2.c:80
msgid ""
"  -h, --help               Print help and exit\n"
"  -V, --version            Print version and exit\n"
msgstr ""
"  -h, --help               вивести довідкові дані і завершити роботу\n"
"  -V, --version            вивести дані щодо версії і завершити роботу\n"

#: src/idn2.c:84
msgid ""
"  -d, --decode             Decode (punycode) domain name\n"
"  -l, --lookup             Lookup domain name (default)\n"
"  -r, --register           Register label\n"
msgstr ""
"  -d, --decode             Декодувати (punycode) доменне ім'я\n"
"  -l, --lookup             Знайти доменне ім'я (типове)\n"
"  -r, --register           Зареєструвати позначку\n"

#: src/idn2.c:89
msgid ""
"  -T, --tr46t              Enable TR46 transitional processing\n"
"  -N, --tr46nt             Enable TR46 non-transitional processing\n"
"      --no-tr46            Disable TR46 processing\n"
msgstr ""
"  -T, --tr46t              Увімкнути перехідну обробку TR46\n"
"  -N, --tr46nt             Увімкнути безперервну обробку TR46\n"
"      --no-tr46            Вимкнути обробку TR46\n"

#: src/idn2.c:94
msgid ""
"      --usestd3asciirules  Enable STD3 ASCII rules\n"
"      --debug              Print debugging information\n"
"      --quiet              Silent operation\n"
msgstr ""

#: src/idn2.c:216
#, c-format
msgid "Charset: %s\n"
msgstr "Кодування: %s\n"

#: src/idn2.c:220
msgid ""
"Type each input string on a line by itself, terminated by a newline "
"character.\n"
msgstr ""
"Виводити кожен рядок у окремому рядку, розділяти рядки символом нового "
"рядка.\n"

#: src/idn2.c:248
msgid "input error"
msgstr "помилка у вхідних даних"
